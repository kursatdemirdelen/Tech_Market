@using Ticaret.Models;
@model IEnumerable<CategoryModel>

<li><a href="/Home/Product"><i class="icon-chevron-right"></i>TÜM ÜRÜNLER </a></li>
@foreach (var item in Model)
{
    <li><a href="/Home/ProductList/@item.Id"><i class="icon-chevron-right"></i>@item.Name<span style="float:right;color:darkslategrey"> @item.Count </span></a></li>
}
